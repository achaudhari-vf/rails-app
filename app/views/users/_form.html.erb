<%= form_with scope: :users, url: users_path, local: true do |f| %>
<p> 
<%= f.label :name %><br/> 
<%= f.text_field :name %>
</p>
  <p> 
  <%= f.label :email %><br/> 
  <%= f.text_field :email %>
</p>
<p>
  <%= f.label :password %><br/> 
  <%= f.text_field :password %> 
</p>

<div class="form-button">
  <%= f.submit %> 
</div>
<% end %>






